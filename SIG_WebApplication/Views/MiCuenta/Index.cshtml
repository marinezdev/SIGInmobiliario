<script>
    function Actualizarpassword()
    {
        if (ValidacionActualizarPassword() == "ok") {
            var obj = {};

            obj['Password'] = $('#MD_Password').val();
            obj['NuPassword'] = $('#MD_NuPassword').val();
            obj['RepPassword'] = $('#MD_RePassword').val();

            var jsonObject = {
                "Usuarios": obj
            };
            $.ajax({
                url: "@Url.Action("Actualizarpassword", "MiCuenta")",
                type: "POST",
                data: JSON.stringify(jsonObject),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                error: function (response) {
                    swal({
                        title: "Algo ocurrió !",
                        text: "Ha ocurrido un problema durante la actualización , inténtalo mas tarde ",
                        icon: "warning",
                        button: "Aceptar",
                    });
                },
                success: function (response) {
                    if (response.Respuesta) {
                        swal({
                            title: "Registro exitoso !",
                            text: 'Se ha actualizado tu password.',
                            icon: "success",
                            button: "Aceptar",
                        })
                            .then((value) => {
                                window.location.href = "@Url.Action("Index", "MiCuenta")";
                        });
                    }
                    else {
                        swal({
                            title: "Algo ocurrió !",
                            text: response.RespuestaText,
                            icon: "warning",
                            button: "Aceptar",
                        });
                    }
                }
            });
        } else {
            swal({
                title: "Algo ocurrió !",
                text: ValidacionActualizarPassword(),
                icon: "warning",
                button: "Aceptar",
            });
        }
    }

    function ValidacionActualizarPassword() {
        var respuesta = "";

        $('#MD_Password').css("border", "2px solid #fff");
        $('#MD_NuPassword').css("border", "2px solid #fff");
        $('#MD_RePassword').css("border", "2px solid #fff");

        if ($('#MD_Password').val().length > 0) {
            if ($('#MD_NuPassword').val().length > 0) {
                if ($('#MD_RePassword').val().length > 0) {
                    if ($('#MD_NuPassword').val() == $('#MD_RePassword').val()) {
                        respuesta = "ok";
                    } else {
                        $('#MD_RePassword').css("border", "2px solid red");
                        respuesta = "Tu nuevo password no coincide ";
                    }
                }
                else {
                    $('#MD_RePassword').css("border", "2px solid red");
                    respuesta = "Coloca tu nuevo password";
                }
            } else {
                $('#MD_NuPassword').css("border", "2px solid red");
                respuesta = "Coloca tu nuevo password";
            }
        } else {
            $('#MD_Password').css("border", "2px solid red");
            respuesta = "Coloca tu Password ";
        }
        return respuesta;
    };


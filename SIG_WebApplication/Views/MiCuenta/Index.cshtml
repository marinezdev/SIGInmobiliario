
@{
    Layout = "~/Views/Shared/_LayoutBusiness.cshtml";
}
<style>
    .nav-pills .nav-link.active, .show > .nav-pills .nav-link {
        color: #fff;
        background-color: #17a2b8 !important;
    }

    .nav-link {
        display: block;
        padding: .5rem 1rem;
        COLOR: #212529;
    }
</style>
<style>
    .cardIn {
        position: relative;
        font-family: 'Proxima Nova',-apple-system,'Helvetica Neue',Helvetica,Roboto,Arial,sans-serif;
        display: -ms-flexbox;
        display: flex;
        -ms-flex-direction: column;
        flex-direction: column;
        min-width: 0;
        word-wrap: break-word;
        background-color: #fff;
        background-clip: border-box;
        border: 1.5px solid rgba(0,0,0,.125);
        border-radius: .25rem;
        -webkit-box-shadow: 0px 0px 5px -1px rgba(0,0,0,0.100);
        -moz-box-shadow: 0px 0px 5px -1px rgba(0,0,0,0.100);
        box-shadow: 0px 0px 5px -1px rgba(0,0,0,0.100);
    }

    .card-titleIn {
        margin-bottom: .75rem;
        font-family: 'Proxima Nova',-apple-system,'Helvetica Neue',Helvetica,Roboto,Arial,sans-serif;
        font-size: 14px;
        font-weight: bold;
    }

    .card-top-bar {
        height: 6px;
        background: #00d2ff;
        background: -webkit-linear-gradient(to right, #3a7bd5, #00d2ff);
        background: linear-gradient(to right, #3a7bd5, #00d2ff);
    }

    .card-top-bar-DU {
        height: 6px;
        background: #FFEB3B;
        background: -webkit-linear-gradient(to right, #3a7bd5, #00d2ff);
        background: linear-gradient(to right, #FFEB3B, #FF9800);
    }

    .card-top-bar-RE {
        height: 6px;
        background: #673AB7;
        background: -webkit-linear-gradient(to right, #3a7bd5, #00d2ff);
        background: linear-gradient(to right, #673AB7, #9C27B0);
    }

    .card-top-bar-Renta {
        height: 6px;
        background: #4CAF50;
        background: -webkit-linear-gradient(to right, #3a7bd5, #00d2ff);
        background: linear-gradient(to right, #4CAF50, #CDDC39);
    }

    .card-top-bar-Vendido {
        height: 6px;
        background: #FFC107;
        background: -webkit-linear-gradient(to right, #3a7bd5, #00d2ff);
        background: linear-gradient(to right, #FFC107, #F44336);
    }

    .card-top-bar-RADM {
        height: 6px;
        background: #E91E63;
        background: -webkit-linear-gradient(to right, #3a7bd5, #00d2ff);
        background: linear-gradient(to right, #E91E63, #F44336);
    }

    .btn {
        display: inline-block;
        font-weight: 400;
        text-align: center;
        white-space: nowrap;
        vertical-align: middle;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
        border: 1px solid transparent;
        padding: .5rem .75rem;
        font-size: 1rem;
        line-height: 1.25;
        border-radius: .25rem;
        transition: all .15s ease-in-out;
        font-family: 'Proxima Nova',-apple-system,'Helvetica Neue',Helvetica,Roboto,Arial,sans-serif;
        border-color: #17a2b8;
        background-color: #17a2b8;
        color: #ffffff;
    }

        .btn:hover {
            border-color: #17a2b8;
            background-color: #f8f9fa;
            color: #17a2b8;
        }
</style>
<br />
<br />
<br />


<!-- Shoping Cart -->
<form class="bg0 p-t-75 p-b-85">
    <div class="container">
        <div class="row">

            <div class="col-md-4">
                <div class="bor10 p-lr-40 p-t-30 p-b-40  p-lr-15-sm">
                    <div class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical" style="color:goldenrod">
                        <a class="nav-link active" id="v-pills-home-tab" data-toggle="pill" href="#v-pills-home" role="tab" aria-controls="v-pills-home" aria-selected="true">@*<i class="fa fa-tasks" aria-hidden="true"></i>*@ Resumen </a>
                        <a class="nav-link" id="v-pills-profile-tab" data-toggle="pill" href="#v-pills-profile" role="tab" aria-controls="v-pills-profile" aria-selected="false">@*<i class="fa fa-newspaper-o" aria-hidden="true"></i>*@ Mis Publicaciones </a>
                        <a class="nav-link" id="v-pills-mess-tab" data-toggle="pill" href="#v-pills-mess" role="tab" aria-controls="v-pills-mess" aria-selected="false">@*<i class="fa fa-paper-plane-o" aria-hidden="true"></i>*@ Mis Mensajes </a>
                        <a class="nav-link" id="v-pills-inmuebles-tab" data-toggle="pill" href="#v-pills-inmuebles" role="tab" aria-controls="v-pills-inmuebles" aria-selected="false">@*<i class="fa fa-database" aria-hidden="true"></i>*@ Inmuebles </a>
                        <a class="nav-link" id="v-pills-datos-tab" data-toggle="pill" href="#v-pills-datos" role="tab" aria-controls="v-pills-datos" aria-selected="false">@*<i class="fa fa-database" aria-hidden="true"></i>*@ Mis Datos </a>
                    </div>
                </div>
            </div>


            <div class="col-md-8">
                <div class="bor10 p-lr-40 p-t-30 p-b-40 m-l-10 m-r-4 m-lr-0-xl p-lr-15-sm">

                    <div class="tab-content" id="v-pills-tabContent">
                        <div class="tab-pane fade show active" id="v-pills-home" role="tabpanel" aria-labelledby="v-pills-home-tab">

                            <h5><strong>Alcance de mis publicaciones</strong> </h5>

                            @*<p>Últimos 7 días</p>*@

                            <hr />

                            <div class="row text-center">
                                <div class="card bg-light mb-5" style="max-width: 30rem;">
                                    <div class="card-header"> </div>
                                    <div class="card-body">
                                        <h1 class="card-title"><i class="fa fa-eye" aria-hidden="true"></i></h1>
                                        <h1 class="card-title">@ViewBag.VieuTotal</h1>
                                        <p class="card-text">Vieron mis anuncios.</p>
                                    </div>
                                </div>
                                <div class="card text-white bg-info mb-5" style="max-width: 30rem;">
                                    <div class="card-header"> </div>
                                    <div class="card-body">
                                        <h1 class="card-title"><i class="fa fa-comments" aria-hidden="true"></i></h1>
                                        <h1 class="card-title">@ViewBag.MensajeTotal</h1>
                                        <p class="card-text">Mensajes recibidos .</p>
                                    </div>
                                </div>
                                <div class="card bg-light mb-5" style="max-width: 30rem;">
                                    <div class="card-header"> </div>
                                    <div class="card-body">
                                        <h1 class="card-title"><i class="fa fa-phone-square" aria-hidden="true"></i></h1>
                                        <h1 class="card-title">@ViewBag.Num</h1>
                                        <p class="card-text">Vieron mi teléfono.</p>
                                    </div>
                                </div>
                                <div class="card text-white bg-info mb-5" style="max-width: 30rem;">
                                    <div class="card-header"> </div>
                                    <div class="card-body">
                                        <h1 class="card-title"><i class="fa fa-building" aria-hidden="true"></i></h1>
                                        <h1 class="card-title">@ViewBag.Total</h1>
                                        <p class="card-text">Inmuebles publicados.</p>
                                    </div>
                                </div>
                            </div>

                            <hr />
                            <h5><strong>Tus inmuebles más vistos.</strong> </h5>
                            <br />
                            <div class="table-responsive">

                                <table class="table table-striped table-hover table-sm">
                                    <thead class="thead-light">
                                        <tr>
                                            <th scope="col">Categoría </th>
                                            <th scope="col">Tipo</th>
                                            <th scope="col">Precio</th>
                                            <th scope="col">Titulo </th>
                                            <th scope="col">Total, visualizaciones</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var Com in ViewBag.ListaInmueblesVistos)
                                        {
                                            <tr>
                                                <th scope="row">@Com.TipoCategoria</th>
                                                <td>@Com.TipoSubCategoria</td>
                                                <td>@Com.PrecioFinalText</td>
                                                <td>@Com.Titulo</td>
                                                <td>@Com.Total</td>
                                            </tr>
                                        }

                                    </tbody>
                                </table>
                            </div>
                            <br>
                            <canvas id="densityChart" width="600" height="200"></canvas>
                            <hr />
                            <h5><strong>Tus inmuebles con mas mensajes.</strong> </h5>
                            <br />
                            <div class="table-responsive">

                                <table class="table table-striped table-hover table-sm">
                                    <thead class="thead-light">
                                        <tr>
                                            <th scope="col">Categoría </th>
                                            <th scope="col">Tipo</th>
                                            <th scope="col">Precio</th>
                                            <th scope="col">Titulo </th>
                                            <th scope="col">Total, mensajes</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var Com in ViewBag.ListaMensajesInmueble)
                                        {
                                            <tr>
                                                <th scope="row">@Com.TipoCategoria</th>
                                                <td>@Com.TipoSubCategoria</td>
                                                <td>@Com.PrecioFinalText</td>
                                                <td>@Com.Titulo</td>
                                                <td>@Com.Total</td>
                                            </tr>
                                        }

                                    </tbody>
                                </table>
                            </div>
                            <br>
                            <canvas id="densityChart2" width="600" height="200"></canvas>
                            <hr />
                            <h5><strong>Tus inmuebles mostrar teléfono.</strong> </h5>
                            <br />
                            <div class="table-responsive">

                                <table class="table table-striped table-hover table-sm">
                                    <thead class="thead-light">
                                        <tr>
                                            <th scope="col">Categoría </th>
                                            <th scope="col">Tipo</th>
                                            <th scope="col">Precio</th>
                                            <th scope="col">Titulo </th>
                                            <th scope="col">Total, mensajes</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var Com in ViewBag.ListaTelefonosInmueble)
                                        {
                                            <tr>
                                                <th scope="row">@Com.TipoCategoria</th>
                                                <td>@Com.TipoSubCategoria</td>
                                                <td>@Com.PrecioFinalText</td>
                                                <td>@Com.Titulo</td>
                                                <td>@Com.Total</td>
                                            </tr>
                                        }

                                    </tbody>
                                </table>
                            </div>
                            <br>
                            <canvas id="densityChart3" width="600" height="200"></canvas>
                            <hr />



                            <div class="row text-center">
                                <div class="col-md-2">
                                </div>

                            </div>
                        </div>
                        <div class="tab-pane fade" id="v-pills-profile" role="tabpanel" aria-labelledby="v-pills-profile-tab">
                            <h5><strong>Publicaciones realizadas</strong> </h5>
                            <p class="card-text"><small class="text-muted">Publicaciones activas con estatus: Publico, Revisión</small></p>
                            <hr />

                            @foreach (var Inmuebles in ViewBag.Inmuebles)
                            {
                                <div class="card" style="height:350px">
                                    <a href="#" class="hov-img0 how-pos5-parent">
                                        <img class="card-img-top " src="@Inmuebles.Imagen" alt="Card image cap">
                                        <div class="flex-col-c-m size-123 bg9 how-pos5">
                                            <span class="ltext-107 cl2 txt-center">
                                                @Inmuebles.Dia
                                            </span>
                                            <span class="stext-109 cl3 txt-center">
                                                @Inmuebles.Mes @Inmuebles.FechaRegistro
                                            </span>
                                        </div>
                                    </a>
                                </div>
                                <div class="card-body bor4">
                                    <h5 class="card-title">@Inmuebles.Titulo</h5>
                                    <textarea rows="5" cols="80" class="form-control" disabled="" placeholder="" id="m_Descripcion">@Inmuebles.Descripcion</textarea>
                                    @*<p class="card-text">@Inmuebles.Descripcion.</p>*@
                                    <p class="card-text"><small class="text-muted">Publicado hace @Inmuebles.Tiempo</small></p>

                                    <div class=" m-t-5 ">
                                        <br />
                                        <button type="button" class="flex-c-m cl7 size-301  bor7 hov-tag1 p-lr-15 trans-04"> Estatus: @Inmuebles.NombreEstatus </button>

                                    </div>
                                    <div class=" flex-sb-m ">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <span class="">
                                                    <span>
                                                        <span class="cl4">Categoría: </span> @Inmuebles.TipoCategoria
                                                        <span class="cl12 m-l-4 m-r-6">|</span>
                                                    </span>

                                                    <span>
                                                        @Inmuebles.SubCategoria, @Inmuebles.Estado
                                                        <span class="cl12 m-l-4 m-r-6">|</span>
                                                    </span>

                                                    <span>
                                                        @Inmuebles.Poblacion
                                                    </span>
                                                </span>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="row">
                                                    <a href="@Url.Action("InmuebleDetalle", "MiCuenta" )?Inm=@Inmuebles.IdCf" class=" cl2 hov-cl1 trans-04 m-tb-5">
                                                        <button type="button" id="" class="flex-c-m cl0 size-125 bg3 bor2 hov-btn3 p-lr-15 trans-04">Editar &nbsp; <i class="fa fa-share-square-o" aria-hidden="true"></i></button>
                                                    </a>
                                                    <a class=" cl2 hov-cl1 trans-04 m-tb-5">
                                                        <button type="button" id="" onclick="DesactivaInmueble(@Inmuebles.Id)" class="flex-c-m cl0 size-125 bg3 bor2 hov-btn3 p-lr-15 trans-04">Desactivar   &nbsp; <i class="fa fa-share-square-o" aria-hidden="true"></i></button>
                                                    </a>
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <hr />
                                                <div class="row">
                                                    <div class="col-md-6 text-center">
                                                        <p class="card-text"><small class="text-muted">Informa a los demás que tu inmueble se encuentra rentado o ya fue vendido</small></p>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <div class="row">
                                                            <a class=" cl2 hov-cl1 trans-04 m-tb-5">
                                                                <button type="button" id="" onclick="RentadoInmueble(@Inmuebles.Id)" class="flex-c-m cl0 size-125 bg3 bor2 hov-btn3 p-lr-15 trans-04" style="background-color:#4CAF50;">Rentado &nbsp; <i class="fa fa-thumbs-o-up" aria-hidden="true"></i></button>
                                                            </a>
                                                            <a class=" cl2 hov-cl1 trans-04 m-tb-5">
                                                                <button type="button" id="" onclick="VendidoInmueble(@Inmuebles.Id)" class="flex-c-m cl0 size-125 bg3 bor2 hov-btn3 p-lr-15 trans-04" style="background-color:#FF9800;">Vendido  &nbsp; <i class="fa fa-thumbs-o-up" aria-hidden="true"></i></button>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                                <br /><br />
                            }
                        </div>
                        <div class="tab-pane fade" id="v-pills-mess" role="tabpanel" aria-labelledby="v-pills-mess-tab">
                            <h5><strong>Tus inmuebles con mas mensajes.</strong> </h5>
                            <br />
                            <div class="table-responsive">

                                <table class="table table-striped table-hover table-sm">
                                    <thead class="thead-light">
                                        <tr>
                                            <th scope="col">Categoría </th>
                                            <th scope="col">Tipo</th>
                                            <th scope="col">Precio</th>
                                            <th scope="col">Titulo </th>
                                            <th scope="col">Mensajes</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var Com in ViewBag.ListaMensajesInmueble)
                                        {
                                            <tr>
                                                <th scope="row">@Com.TipoCategoria</th>
                                                <td>@Com.TipoSubCategoria</td>
                                                <td>@Com.PrecioFinalText</td>
                                                <td>@Com.Titulo</td>
                                                <td>
                                                    <button type="button" id="BtnPublicar" onclick="ContalMensajesInmueble(@Com.Id)" class="flex-c-m cl0 size-125 bg3 bor2 hov-btn3 p-lr-15 trans-04 js-show-modal1"> Mostrar </button>
                                                </td>
                                            </tr>
                                        }

                                    </tbody>
                                </table>
                            </div>
                            <br>
                        </div>
                        <div class="tab-pane fade" id="v-pills-inmuebles" role="tabpanel" aria-labelledby="v-pills-inmuebles-tab">
                            <h5><strong>Inmuebles registrados </strong> </h5>
                            <p class="card-text">
                                <small class="text-muted">
                                    Inmuebles con estatus: Rechazo por administración,
                                    Revisión,
                                    Publico,
                                    Desactivado por usuario,
                                    Vendido y
                                    Rentado
                                </small>
                            </p>
                            <hr />
                            @foreach (var Inmuebles in ViewBag.InmueblesLitado)
                            {
                                <div class="card mb-3 cardIn" style="max-width: 100%;">
                                    <div class="row no-gutters">
                                        <div class="col-md-4">
                                            <img src="@Inmuebles.Imagen" class="card-img" />
                                        </div>
                                        <div class="col-md-8">
                                            <div class="card-body">
                                                <h5 class="card-title card-titleIn ">@Inmuebles.Titulo</h5>
                                                <p class="card-text" style="font-style: oblique;">@Inmuebles.SubCategoria - @Inmuebles.TipoCategoria</p>
                                                <p class="card-text">@Inmuebles.Descripcion</p>
                                                <p class="card-text"><small class="text-muted">@Inmuebles.Estado, @Inmuebles.Poblacion </small></p>
                                                <p class="card-text"><small class="text-muted">@Inmuebles.Tiempo</small></p>
                                                <br />
                                                <button type="button" class="flex-c-m cl7 size-301  bor7 hov-tag1 p-lr-15 trans-04"> Estatus: @Inmuebles.NombreEstatus </button>
                                                <hr />
                                                <div class="text-right">
                                                    @if (Inmuebles.NombreEstatus == "Desactivado por usuario")
                                                    {
                                                        <a class="btn my-2 my-sm-0 col-md-3" href="@Url.Action("InmuebleDetalle", "MiCuenta" )?Inm=@Inmuebles.IdCf"><i class="fa fa-share" aria-hidden="true"></i> Editar </a>
                                                        <button type="button" id="" class="btn my-2 my-sm-0 col-md-3" onclick="EliminarInmueble(@Inmuebles.Id)"><i class="fa fa-share" aria-hidden="true"></i> Eliminar </button>
                                                        <button type="button" id="" class="btn my-2 my-sm-0 col-md-3" onclick="PublicarInmueble(@Inmuebles.Id)"><i class="fa fa-share" aria-hidden="true"></i> Publicar </button>
                                                    }
                                                    else if (Inmuebles.NombreEstatus == "Revisión")
                                                    {
                                                        <button type="button" id="" class="btn my-2 my-sm-0 col-md-3" onclick="EliminarInmueble(@Inmuebles.Id)"><i class="fa fa-share" aria-hidden="true"></i> Eliminar </button>
                                                    }
                                                    else if (Inmuebles.NombreEstatus == "Rentado")
                                                    {
                                                        <a class="btn my-2 my-sm-0 col-md-3" href="@Url.Action("InmuebleDetalle", "MiCuenta" )?Inm=@Inmuebles.IdCf"><i class="fa fa-share" aria-hidden="true"></i> Editar </a>
                                                        <button type="button" id="" class="btn my-2 my-sm-0 col-md-3" onclick="EliminarInmueble(@Inmuebles.Id)"><i class="fa fa-share" aria-hidden="true"></i> Eliminar </button>
                                                        <button type="button" id="" class="btn my-2 my-sm-0 col-md-3" onclick="PublicarInmueble(@Inmuebles.Id)"><i class="fa fa-share" aria-hidden="true"></i> Publicar </button>
                                                    }
                                                    else if (Inmuebles.NombreEstatus == "Vendido")
                                                    {
                                                        <a class="btn my-2 my-sm-0 col-md-3" href="@Url.Action("InmuebleDetalle", "MiCuenta" )?Inm=@Inmuebles.IdCf"><i class="fa fa-share" aria-hidden="true"></i> Editar </a>
                                                        <button type="button" id="" class="btn my-2 my-sm-0 col-md-3" onclick="EliminarInmueble(@Inmuebles.Id)"><i class="fa fa-share" aria-hidden="true"></i> Eliminar </button>
                                                        <button type="button" id="" class="btn my-2 my-sm-0 col-md-3" onclick="PublicarInmueble(@Inmuebles.Id)"><i class="fa fa-share" aria-hidden="true"></i> Publicar </button>
                                                    }
                                                    else if (Inmuebles.NombreEstatus == "Rechazo por administración")
                                                    {
                                                        <button type="button" id="" class="btn my-2 my-sm-0 col-md-3" onclick="EliminarInmueble(@Inmuebles.Id)"><i class="fa fa-share" aria-hidden="true"></i> Eliminar </button>
                                                    }
                                                    else
                                                    {
                                                        <a class="btn my-2 my-sm-0 col-md-3" href="@Url.Action("InmuebleDetalle", "MiCuenta" )?Inm=@Inmuebles.IdCf"><i class="fa fa-share" aria-hidden="true"></i> Editar </a>
                                                        <button type="button" id="" class="btn my-2 my-sm-0 col-md-3" onclick="EliminarInmueble(@Inmuebles.Id)"><i class="fa fa-share" aria-hidden="true"></i> Eliminar </button>
                                                    }

                                                </div>
                                            </div>
                                        </div>
                                        @if (Inmuebles.NombreEstatus == "Desactivado por usuario")
                                        {
                                            <div class="col-md-12 card-top-bar-DU">

                                            </div>
                                        }
                                        else if (Inmuebles.NombreEstatus == "Revisión")
                                        {
                                            <div class="col-md-12 card-top-bar-RE">

                                            </div>
                                        }
                                        else if (Inmuebles.NombreEstatus == "Rentado")
                                        {
                                            <div class="col-md-12 card-top-bar-Renta">

                                            </div>
                                        }
                                        else if (Inmuebles.NombreEstatus == "Vendido")
                                        {
                                            <div class="col-md-12 card-top-bar-Vendido">

                                            </div>
                                        }
                                        else if (Inmuebles.NombreEstatus == "Rechazo por administración")
                                        {
                                            <div class="col-md-12 card-top-bar-RADM">

                                            </div>
                                        }
                                        else
                                        {
                                            <div class="col-md-12 card-top-bar">

                                            </div>
                                        }

                                    </div>
                                </div>
                            }
                        </div>
                        <div class="tab-pane fade" id="v-pills-datos" role="tabpanel" aria-labelledby="v-pills-datos-tab">
                            <h5><strong>Información de usuario </strong> </h5>
                            <hr />
                            <div class="col-md-12">
                                <div class="row">
                                    <div class="col-md-6">

<!-- Product Detail -->
<section class="sec-product-detail bg0 p-t-6 p-b-6">
    <div class="container">
        <div class="row" style="display: block;">
            <div class="col-md-6 col-lg-7 p-b-30">

                <div class="alternative-cta-container">
                    <button data-toggle="modal" data-target="#ModalLlamada">Quiero que me llamen</button>
                    <button data-toggle="modal" data-target="#ModalConsulta">Enviar consulta</button>
                    <button data-toggle="modal" data-target="#ModalAgendar">Agendar visita</button>
                </div>
            </div>
        </div>
        <div class="row">







            <div class="col-md-6 col-lg-7 p-b-30">
                <div class="p-l-25 p-r-30 p-lr-0-lg">
                    @{
                        if (ViewBag.Imgs.Count > 5)
                        {
                            <div id="carousel-example-1" class="single-product-slider carousel slide" data-ride="carousel" data-interval="3000">
                                <div class="carousel-inner" role="listbox">
                                    @{
                                        int acti = 0;
                                        foreach (var cat in ViewBag.Imgs)
                                        {
                                            if (acti == 0)
                                            {
                                                <div class="carousel-item active"> <img class="d-block w-100" src="http://@cat.Descripcion"> </div>

                                                acti = acti + 1;
                                            }
                                            else
                                            {
                                                <div class="carousel-item"> <img class="d-block w-100" src="http://@cat.Descripcion" > </div>
                                            }
                                        }
                                    }
                                </div>
                                <a class="carousel-control-prev" href="#carousel-example-1" role="button" data-slide="prev">
                                    <i class="fa fa-angle-left" aria-hidden="true"></i>
                                    <span class="sr-only">Previous</span>
                                </a>
                                <a class="carousel-control-next" href="#carousel-example-1" role="button" data-slide="next">
                                    <i class="fa fa-angle-right" aria-hidden="true"></i>
                                    <span class="sr-only">Next</span>
                                </a>
                                <ol class="carousel-indicators">
                                    @{
                                        int acti2 = 0;
                                        int cont = 0;
                                        foreach (var cat in ViewBag.Imgs)
                                        {
                                            if (acti2 == 0)
                                            {
                                                <li data-target="#carousel-example-1" data-slide-to="@cont" class="active">
                                                    <img class="d-block w-100 img-fluid" src="http://@cat.Descripcion" alt="" />
                                                </li>
                                                acti2 = acti2 + 1;
                                            }
                                            else
                                            {
                                                <li data-target="#carousel-example-1" data-slide-to="@cont">
                                                    <img class="d-block w-100 img-fluid" src="http://@cat.Descripcion" alt="" />
                                                </li>
                                            }
                                        }
                                    }
                                </ol>
                            </div>
                        }
                        else
                        {
                            <div class="wrap-slick3 flex-sb flex-w">
                                <div class="wrap-slick3-dots"></div>
                                <div class="wrap-slick3-arrows flex-sb-m flex-w"></div>

                                <div class="slick3 gallery-lb">
                                    @{
                                        foreach (var cat in ViewBag.Imgs)
                                        {
                                            <div class="item-slick3" data-thumb="http://@cat.Descripcion">
                                                <div class="wrap-pic-w pos-relative">
                                                    <img src="http://@cat.Descripcion" alt="IMG-PRODUCT">

                                                    <a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="http://@cat.Descripcion">
                                                        <i class="fa fa-expand"></i>
                                                    </a>
                                                </div>
                                            </div>
                                        }
                                    }
                                </div>
                            </div>
                        }
                    }
                </div>
            </div>

            <div class="col-md-6 col-lg-5 p-b-30">

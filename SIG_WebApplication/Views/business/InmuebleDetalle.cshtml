<!-- MODALS QUIERO QUE ME LLAMEN --->
<div class="modal fade" id="ModalLlamada" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header text-center Header-Modal">
                <h5 class="modal-title text-center" id="exampleModalLabel">Quiero que me llamen</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <label>Email  </label>
                <div class="bor8 bg0 m-b-2 m-t-2 col-md-12">
                    <input class="stext-111 size-116 " type="email" name="Email" id="ML_Email" placeholder="Email">
                </div>
                <br />
                <div class="row">
                    <div class="col-md-6">
                        <label>Nombre  </label>
                        <div class="bor8 bg0 m-b-2 m-t-2 col-md-12">
                            <input class="stext-111 size-116 " type="text" name="Nombre" id="ML_NombreContacto" placeholder="Nombre">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label>Teléfono  </label>
                        <div class="bor8 bg0 m-b-2 m-t-2 col-md-12">
                            <input class="stext-111 size-116 " type="tel" name="Telefono" id="ML_Telefono" placeholder="Teléfono">
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-12 text-center">
                        <button type="button" class="btn-header-modal col-md-5" onclick="JsLlamada(@ViewBag.Id)">Enviar </button>
                    </div>
                    <div class="col-md-12 text-center">
                        <br />
                        <p style="font-size:12px">Al enviar estás aceptando los <a href="#"><strong>Términos y condiciones de Uso </strong></a></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

